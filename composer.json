{
    "name": "sourcetoad/enhanced-resources",
    "description": "Laravel's API Resources enhanced.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Jones",
            "email": "jason.erik.jones@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sourcetoad\\EnhancedResources\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sourcetoad\\EnhancedResources\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^7.0|^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^4.2",
        "orchestra/testbench": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sourcetoad\\EnhancedResources\\ServiceProvider"
            ],
            "aliases": {
                "ResourceEnhancements": "Sourcetoad\\EnhancedResources\\Support\\Facades\\ResourceEnhancements"
            }
        }
    }
}
